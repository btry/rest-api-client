{
  "name": "glpi-project/rest-api-client",
  "type": "library",
  "description": "Client for GLPI rest API",
  "keywords": [
    "GLPI",
    "rest",
    "api",
    "client"
  ],
  "homepage": "https://github.com/glpi-project/rest-api-client",
  "license": "GPL-3.0+",
  "authors": [
    {
      "name": "Thierry Bugier",
      "email": "tbugier@teclib.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "atoum/atoum": "^3.1",
    "atoum/stubs": "^2.5",
    "atoum/json-schema-extension": "^2.1",
    "glpi-project/tools": "^0.1.2"
  },
  "autoload": {
    "psr-4": {
      "GlpiProject\\API\\Rest\\": "src/GlpiProject/API/Rest/"
    },
    "exclude-from-classmap": ["tests/"]
  },
  "autoload-dev": {
    "psr-4": {
      "GlpiProject\\API\\Rest\\tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
